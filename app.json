{
  "expo": {
    "name": "lazyfood-front",
    "slug": "lazyfood-front",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "lazyfoodfront",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.lazyfood.app",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      "./plugins/android-network-config"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3e3028bb-5f4d-48ef-ab1a-ab0980cf1165"
      }
    },
    "owner": "tito2"
  }
}
